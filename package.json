{
  "name": "cep-former22-middleware",
  "version": "1.0.0",
  "description": "CEP Middleware for PeopleSoft and Former22",
  "author": "konsept.ch",
  "license": "SEE LICENSE IN LICENSE.txt",
  "private": true,
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.15.0"
  },
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "start": "pm2 start ecosystem.config.cjs && pm2 logs",
    "start-prod": "pm2 start ecosystem.config.cjs --env production && pm2 logs",
    "stop": "pm2 stop middleware",
    "monitor": "pm2 monit",
    "logs": "pm2 logs",
    "prettier:check": "prettier . --ignore-unknown --check",
    "prettier:fix": "prettier . --ignore-unknown --write",
    "start:local": "cross-env CLAROLINE_API_URL=http://localhost/apiv2/ npm start"
  },
  "dependencies": {
    "@prisma/client": "4.3.1",
    "cors": "2.8.5",
    "docxtemplater": "3.31.3",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "form-data": "4.0.0",
    "js-yaml": "4.1.0",
    "libreoffice-convert": "1.4.1",
    "luxon": "3.0.4",
    "mailgun.js": "8.0.1",
    "multer": "1.4.5-lts.1",
    "nanoid": "4.0.0",
    "node-fetch": "3.2.10",
    "pizzip": "3.1.3",
    "pm2": "5.2.0",
    "prisma": "4.3.1",
    "swagger-jsdoc": "7.0.0-rc.6",
    "swagger-ui-express": "4.5.0",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1"
  }
}
