{
  "name": "cep-former22-server",
  "version": "1.0.0",
  "description": "CEP Middleware server for PeopleSoft and Former22",
  "author": "konsept.ch",
  "license": "SEE LICENSE IN LICENSE.txt",
  "private": true,
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=8.19.3"
  },
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "start": "db-migrate up && pm2 start ecosystem.config.cjs && pm2 logs",
    "start-prod": "db-migrate up && pm2 start ecosystem.config.cjs --env production && pm2 logs",
    "ts-node": "node -r ts-node/register/transpile-only --loader ts-node/esm/transpile-only --unhandled-rejections=strict --experimental-specifier-resolution=node ./bin/www.ts",
    "stop": "pm2 stop server",
    "monitor": "pm2 monit",
    "logs": "pm2 logs",
    "invoices:migrate": "node ./scripts/invoices.js",
    "migrate:create": "db-migrate create",
    "migrate:up": "db-migrate up",
    "schema": "prisma db pull && prisma generate",
    "prettier:check": "prettier . --ignore-unknown --check",
    "prettier:fix": "prettier . --ignore-unknown --write",
    "eslint:check": "eslint --ext .js,.jsx,.ts,.tsx . --report-unused-disable-directives",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "typescript:check": "tsc --noEmit",
    "start:local": "cross-env CLAROLINE_API_URL=http://localhost/apiv2/ npm start",
    "ci:check": "npm run prettier:check && npm run eslint:check && npm run typescript:check",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "@swc/core": "1.3.32",
    "@swc/wasm": "1.3.32",
    "@types/cookie": "0.5.1",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/js-yaml": "4.0.5",
    "@types/luxon": "3.2.0",
    "@types/morgan": "1.9.4",
    "@types/multer": "1.4.7",
    "@types/node": "18.11.18",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/uuid": "9.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "db-migrate": "1.0.0-beta.18",
    "db-migrate-mysql": "^2.2.0",
    "debug": "4.3.4",
    "docxtemplater": "3.34.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "form-data": "4.0.0",
    "helmet": "6.0.1",
    "js-yaml": "4.1.0",
    "libreoffice-convert": "1.4.1",
    "luxon": "3.2.1",
    "mailgun.js": "8.0.6",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "nanoid": "4.0.0",
    "node-fetch": "3.3.0",
    "pdf-lib": "^1.17.1",
    "pizzip": "3.1.4",
    "pm2": "5.4.0",
    "prisma": "4.9.0",
    "swagger-jsdoc": "7.0.0-rc.6",
    "swagger-ui-express": "4.6.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "uuid": "9.0.0",
    "winston": "3.8.2",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "cross-env": "7.0.3",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3"
  }
}
