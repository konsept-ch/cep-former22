  @@index([template_id], map: "IDX_7A1CAEBE5DA0FB8")
  @@index([workspace_id], map: "IDX_7A1CAEBE82D40A1F")
  @@index([issuer_id], map: "IDX_7A1CAEBEBB9D6FEE")
}

model claro__open_badge_endorsement {
  id                                    Int                                    @id @default(autoincrement())
  issuer_id                             Int?
  verification_id                       Int?
  claim                                 String                                 @db.LongText
