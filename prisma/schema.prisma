generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model badgeclass_group {
  badgeclass_id                 Int
  group_id                      Int
  claro__open_badge_badge_class claro__open_badge_badge_class @relation(fields: [badgeclass_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_1F0F6E8998CCE3D1")
  claro_group                   claro_group                   @relation(fields: [group_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_1F0F6E89FE54D947")

  @@id([badgeclass_id, group_id])
  @@index([badgeclass_id], map: "IDX_1F0F6E8998CCE3D1")
  @@index([group_id], map: "IDX_1F0F6E89FE54D947")
}

model badgeclass_user {
  badgeclass_id                 Int
  user_id                       Int
  claro__open_badge_badge_class claro__open_badge_badge_class @relation(fields: [badgeclass_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_45DE1F8F98CCE3D1")
  claro_user                    claro_user                    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_45DE1F8FA76ED395")

  @@id([badgeclass_id, user_id])
  @@index([badgeclass_id], map: "IDX_45DE1F8F98CCE3D1")
  @@index([user_id], map: "IDX_45DE1F8FA76ED395")
}

model claro__location {
  id                                Int                                 @id @default(autoincrement())
  name                              String                              @db.VarChar(255)
  type                              Int
  address_street2                   String?                             @db.VarChar(255)
  address_postal_code               String?                             @db.VarChar(255)
  address_city                      String?                             @db.VarChar(255)
  address_country                   String?                             @db.VarChar(255)
  latitude                          Float?
  longitude                         Float?
  phone                             String?                             @db.VarChar(255)
  uuid                              String                              @unique(map: "UNIQ_24C849F7D17F50A6") @db.VarChar(36)
  description                       String?                             @db.LongText
  poster                            String?                             @db.VarChar(255)
  address_street1                   String?                             @db.VarChar(255)
  address_state                     String?                             @db.VarChar(255)
  thumbnail                         String?                             @db.VarChar(255)
  claro__location_organization      claro__location_organization[]
  claro_booking_material            claro_booking_material[]
  claro_cursusbundle_course_session claro_cursusbundle_course_session[]
  claro_location_room               claro_location_room[]
  claro_planned_object              claro_planned_object[]
  group_location                    group_location[]
  user_location                     user_location[]
}

model claro__location_organization {
  organization_id     Int
  location_id         Int
  claro__location     claro__location     @relation(fields: [location_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_C4EBDE064D218E")
  claro__organization claro__organization @relation(fields: [organization_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "FK_C4EBDE032C8A3DE")

  @@id([organization_id, location_id])
  @@index([organization_id], map: "IDX_C4EBDE032C8A3DE")
  @@index([location_id], map: "IDX_C4EBDE064D218E")
}

model claro__open_badge_assertion {
  id                                    Int                                    @id @default(autoincrement())
  recipient_id                          Int?
  badge_id                              Int?
  verification_id                       Int?
  issuedOn                              DateTime                               @db.DateTime(0)
  image                                 String?                                @db.LongText
  narrative                             String?                                @db.LongText
  revoked                               Boolean
  revocationReason                      String?                                @db.LongText
  uuid                                  String                                 @unique(map: "UNIQ_B6E0ABADD17F50A6") @db.VarChar(36)
