  claro_user                            claro_user?                            @relation(fields: [recipient_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_B6E0ABADE92F8F78")
  claro__open_badge_verification_object claro__open_badge_verification_object? @relation(fields: [verification_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_B6E0ABAD1623CB0A")
  claro__open_badge_evidence            claro__open_badge_evidence[]
  revocationlist_assertion              revocationlist_assertion[]

  @@index([verification_id], map: "IDX_B6E0ABAD1623CB0A")
  @@index([recipient_id], map: "IDX_B6E0ABADE92F8F78")
  @@index([badge_id], map: "IDX_B6E0ABADF7A2C2FC")
}

model claro__open_badge_badge_class {
  id                          Int                           @id @default(autoincrement())
  issuer_id                   Int?
  workspace_id                Int?
  name                        String                        @db.VarChar(255)
  description                 String?                       @db.LongText
  image                       String                        @db.VarChar(255)
  criteria                    String?                       @db.LongText
  enabled                     Boolean?
  durationValidation          Int?
  hideRecipients              Boolean
  created                     DateTime                      @db.DateTime(0)
  updated                     DateTime                      @db.DateTime(0)
  uuid                        String                        @unique(map: "UNIQ_7A1CAEBED17F50A6") @db.VarChar(36)
  color                       String?                       @db.VarChar(255)
  issuingPeer                 Boolean
  template_id                 Int?
  claro__organization         claro__organization?          @relation(fields: [issuer_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_7A1CAEBEBB9D6FEE")
  claro_template              claro_template?               @relation(fields: [template_id], references: [id], onUpdate: NoAction, map: "FK_7A1CAEBE5DA0FB8")
  claro_workspace             claro_workspace?              @relation(fields: [workspace_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "FK_7A1CAEBE82D40A1F")
