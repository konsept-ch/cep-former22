  @@index([assertion_id], map: "IDX_6F68173245A6843")
  @@index([resourceEvidence_id], map: "IDX_6F6817330E7C438")
  @@index([rule_id], map: "IDX_6F68173744E0351")
  @@index([user_id], map: "IDX_6F68173A76ED395")
